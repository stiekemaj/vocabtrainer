<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="1.0.001" author="jeroen.stiekema">
        <comment>Create axon event tables</comment>
        <createTable tableName="USERS">
            <column name="USER_ID" type="varchar2(255)">
                <constraints primaryKeyName="PK_USERS" primaryKey="true"/>
            </column>
            <column name="USERNAME" type="varchar2(255)">
                <constraints nullable="false"/>
            </column>
            <column name="PASSWORD" type="varchar2(255)">
                <constraints nullable="false"/>
            </column>
            <column name="NAME" type="varchar2(512)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>